{
  "name": "exegesis-express-example",
  "version": "1.0.1-4",
  "description": "",
  "scripts": {
    "build": "bash ./scripts/build.bash",
    "build:doc": "rm -rf openapi && mkdir openapi && cd openapi && yarn build:doc:api",
    "build:doc:api": "speccy resolve ./src/openapi/api/index.yml -o ./openapi/api.yaml",
    "build:type": "openapi --input ./openapi/api.yaml --output ./generated --client axios",
    "dev": "nodemon -e yml,ts,json ./",
    "next-version": "npx next-standard-version",
    "release:production": "npx standard-version --message 'chore(release-production): %s\n\n[skip ci]' --no-verify",
    "release:staging": "npx standard-version --message 'chore(release-staging): %s\n\n[skip ci]' --no-verify --prerelease --skip.changelog=true --skip.tag=true",
    "start": "node -r ./tsconfig-paths-bootstrap.js ./dist/",
    "lint:swagger": "ts-node ./app/swagger/index.swagger.ts | speccy lint",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "exegesis-express": "^2.0.0",
    "express": "^4.17.1",
    "flat": "^5.0.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "pino": "^5.16.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/body-parser": "^1.17.1",
    "@types/convict": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "6.1.3",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pino": "^5.15.3",
    "@types/prettier": "1.19.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "lint-staged": "9.5.0",
    "nodemon": "^1.19.3",
    "openapi-typescript-codegen": "^0.19.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^1.19.1",
    "speccy": "^0.11.0",
    "standard-version": "^7.1.0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
